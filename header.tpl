<h2 class="w3-block w3-dark-grey w3-center w3-padding">PastaRater OneDotOh</h2>
<div class="w3-row w3-margin-top">
    <form class="w3-third" action="/login" method="Get">
        <div class="w3-container">
            <div class="w3-gray">
                <h2><input class="w3-btn w3-block w3-center w3-padding-small" type="submit" value="Login"/></h2>
            </div>
        </div>
    </form>
    <form class="w3-third" action="/graph_pastas" method="Get">
        <div class="w3-container">
            <div class="w3-blue-grey">
                <h2><input class="w3-btn w3-block w3-center w3-padding-small" type="submit" value="Graphing" /></h2>
            </div>
        </div>
    </form>
    <form class="w3-third" action="/logout" method="Get">
        <div class="w3-container">
            <div class="w3-gray">
                <h2><input class="w3-btn w3-block w3-center w3-padding-small" type="submit" value="Logout" /></h2>
            </div>
        </div>
    </form>
</div>